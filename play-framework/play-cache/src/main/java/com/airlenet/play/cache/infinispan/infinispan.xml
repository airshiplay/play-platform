<?xml version="1.0" encoding="UTF-8"?>
<infinispan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:infinispan:config:8.2 http://www.infinispan.org/schemas/infinispan-config-8.2.xsd"
	xmlns="urn:infinispan:config:8.2">

	<cache-container default-cache="default">
		<jmx duplicate-domains="true" />
		<local-cache name="xml-configured-cache">
			<eviction strategy="LIRS" max-entries="10000" />
		</local-cache>
	</cache-container>

</infinispan>
